<div id="main-nav" ng-controller="MainMenuCtrl">
  <h1 id="logo-wrap">
    <a href="<%- base %>" id="logo">Hexo</a>
  </h1>
  <ul id="main-nav-list">
    <li class="main-nav-item" ng-repeat="item in menu">
      <a href="<%- base %>{{ item.href }}" class="main-nav-link" ng-class="{current: data.menu == item.href}" ng-bind="item.title"></a>
    </li>
  </ul>
</div>